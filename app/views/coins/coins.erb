<h1><%= "$ #{@sum}" %></h1>
<% @coins.each { |coin| %>
<li><%= "$ #{coin.to_dollar.round(3)} #{coin.name}"%>
    <form action="/coins/<%= coin.slug %>/edit">
      <input type="submit" value="Edit" />
    </form>
</li>
<% } %>

